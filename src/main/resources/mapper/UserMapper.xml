<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.chnsys.springbootdemo.mapper.UserMapper">

  <insert id="insertUser" parameterType="cn.com.chnsys.springbootdemo.pojo.User" >
    insert into user (uid,username,password,nickname,email,state,code)
    values (#{uid},#{username},#{password},#{nickname},#{email},#{state},#{code})
  </insert>
</mapper>